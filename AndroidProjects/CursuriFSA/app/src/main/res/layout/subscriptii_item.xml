<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_marginTop="30dp">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textview_subscriptii_nume"
        android:textStyle="normal"
        android:fontFamily="@font/roboto_fonts_family"
        android:textSize="18sp"
        android:textColor="#333B48"
        android:text="Nume materie"
        android:layout_gravity="start|center"
        android:layout_marginStart="30dp"
        android:layout_weight="0.9"/>
    <ImageView
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:id="@+id/imageview_subscriptii_dismiss"
        android:src="@drawable/ic_subscriptii_dismiss"
        android:layout_weight="0.1"
        android:layout_gravity="right|end"
        />





</LinearLayout>